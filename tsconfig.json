  "compilerOptions": {
    "strict": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["es2015", "es2020", "es2016", "es2017", "es2018", "es2019", "DOM"],
    "target": "ES5",
    "outDir": "./dist",
    "esModuleInterop": true
